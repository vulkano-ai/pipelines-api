    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@semantic-release/gitlab": "^8.0.1",
    "semantic-release": "^19.0.2",
